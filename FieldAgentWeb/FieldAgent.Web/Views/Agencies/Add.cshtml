@model FieldAgent.Core.Entities.Agency

<h1>Add New Agency</h1>
<div class="row">
    <div class="col-sm-12 col-md-6 col-lg-4">
        <form asp-controller="Agencies" asp-action="Add" method="post">
            <div class="mb-3">
                <label asp-for="ShortName" class="form-label">Short Name</label>
                <input type="text" class="form-control" id="FirstName" asp-for="ShortName">
                <label asp-for="LongName" class="form-label">Long Name</label>
                <input type="text" class="form-control" id="LastName" asp-for="LongName">
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>